   "source": [
    "def solution(number, k):\n",
    "    number = list(map(int, list(number)))\n",
    "    number.sort(reverse=True)\n",
    "    answer = number[:k]\n",
    "    answer = list(map(str, answer))\n",
    "    return \"\".join(answer)"
   ]
  },
