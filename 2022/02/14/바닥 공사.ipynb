   "source": [
    "# 1부터 시작이라 1001까지\n",
    "d = [0] * 1001\n",
    "d[1] = 1\n",
    "d[2] = 3\n",
    "\n",
    "for i in range(3, n + 1):\n",
    "    # 뒤에 한개에다가 뒤에 두개를 곱한것을 더한다\n",
    "    d[i] = (d[d - 1] + 2 * d[i - 2]) % 796796\n",
    "\n",
    "print(d[n])"
   ],
