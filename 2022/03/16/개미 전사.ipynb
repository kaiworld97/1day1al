    "n = int(input())\n",
    "l = list(map(int, input().split()))\n",
    "# 1 3 3 8\n",
    "d = [0] * n\n",
    "\n",
    "d[0] = l[0]\n",
    "d[1] = max(l[0], l[1])\n",
    "for i in range(2, n):\n",
    "    d[i] = max(d[i - 1], d[i - 2] + l[i])\n",
    "\n",
    "print(d[n - 1])"
   ]
