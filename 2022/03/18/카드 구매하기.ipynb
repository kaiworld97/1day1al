    "l = [0] + list(map(int, input().split()))\n",
    "d = [0] * (n + 1)\n",
    "\n",
    "for i in range(1, n + 1):\n",
    "    for j in range(1, i + 1):\n",
    "        d[i] = max(d[i], d[i - j] + l[j])"
