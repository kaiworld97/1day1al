   "source": [
    "c = int(input())\n",
    "l = [list(map(int, input().split())) for _ in range(c)]\n",
    "\n",
    "for i in l:\n",
    "    n = i.pop(0)\n",
    "    s = sum(i)\n",
    "    cc = 0\n",
    "    for j in i:\n",
    "        if j > s/n:\n",
    "            cc += 1\n",
    "    k = cc/n * 100\n",
    "    print(f'{k:.3f}%')\n"
   ]
  }
 ],
